<FindBugsFilter>
    <Match>
        <Source name="~.*Test\.java" />
        <Bug code="RV, UMAC"/>
    </Match>
    <Match>
        <Source name="~.*GameMenuController\.java" />
        <Class name="~.*GameMenuController" />
        <Bug pattern="DM_EXIT"/>
    </Match>
    <Match>
        <Package name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP, EI_EXPOSE_REP2"/>
    </Match>
</FindBugsFilter>
